{
  "name": "yd-sdk",
  "version": "1.1.23",
  "description": "Typed isomorphic Yandex Disk SDK",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx esbuild index.ts --bundle --outfile=dist/index.js --platform=neutral",
    "build-schema": "npx reqsrv-sdk --ns=YD --error=src/types/entities/Error.ts",
    "prepublishOnly": "npm run build",
    "preversion": "npx npm-run-all typecheck build",
    "typecheck": "tsc --noEmit"
  },
  "author": "axtk",
  "license": "ISC",
  "keywords": [
    "yandex-disk",
    "api",
    "typed api",
    "sdk",
    "typed sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axtk/yd-sdk.git"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "reqsrv": "^4.6.4"
  }
}
