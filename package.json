{
  "name": "yd-sdk",
  "version": "1.2.0",
  "description": "Typed isomorphic Yandex Disk SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npx npm-run-all clean compile",
    "build-schema": "npx reqsrv-sdk --ns=YD --error=src/types/entities/Error.ts",
    "clean": "node -e \"require('node:fs').rmSync('dist', { force: true, recursive: true });\"",
    "compile": "npx esbuild index.ts --bundle --outdir=dist --platform=neutral",
    "prepublishOnly": "npx npm-run-all build types",
    "preversion": "npx npm-run-all typecheck shape build types",
    "shape": "npx codeshape",
    "typecheck": "tsc --noEmit",
    "types": "tsc --emitDeclarationOnly"
  },
  "author": "axtk",
  "license": "MIT",
  "keywords": [
    "yandex-disk",
    "api",
    "typed api",
    "sdk",
    "typed sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axtk/yd-sdk.git"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@t8/sdk-factory": "^1.0.8"
  }
}
